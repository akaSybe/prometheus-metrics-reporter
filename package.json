{
  "name": "@akasybe/prometheus-metrics",
  "version": "1.1.0",
  "description": "Generates prometheus metrics using tasks from config file",
  "main": "index.js",
  "bin": {
    "prometheus-metrics": "index.js"
  },
  "scripts": {
    "test": "jest",
    "semantic-release": "semantic-release",
    "tasks": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akaSybe/prometheus-metrics.git"
  },
  "keywords": [
    "prometheus",
    "metrics"
  ],
  "author": "Aleksandr Shestakov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akaSybe/prometheus-metrics/issues"
  },
  "homepage": "https://github.com/akaSybe/prometheus-metrics#readme",
  "files": [
    "lib",
    "index.js"
  ],
  "dependencies": {
    "commander": "^4.1.1",
    "glob": "^7.1.6",
    "gzip-size": "^5.1.1",
    "prettycli": "^1.4.3",
    "prom-client": "^12.0.0"
  },
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "cosmiconfig": "^6.0.0",
    "cz-conventional-changelog": "^3.1.0",
    "execa": "^4.0.0",
    "husky": "^4.2.3",
    "jest": "^25.4.0",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "semantic-release": "^17.0.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
