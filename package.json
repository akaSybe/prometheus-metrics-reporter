{
  "name": "prometheus-metrics",
  "version": "0.0.0-development",
  "description": "Generate file with prometheus-friendly metrics based on config",
  "main": "index.js",
  "scripts": {
    "test": "echo \"No tests\" && exit 0",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akaSybe/prometheus-metrics.git"
  },
  "keywords": [
    "prometheus",
    "metrics"
  ],
  "author": "Aleksandr Shestakov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akaSybe/prometheus-metrics/issues"
  },
  "homepage": "https://github.com/akaSybe/prometheus-metrics#readme",
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "cz-conventional-changelog": "^3.1.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "semantic-release": "^17.0.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
